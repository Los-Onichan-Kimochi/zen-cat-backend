{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "./main",
    "healthcheckPath": "/health-check/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 5
  },
  "variables": {
    "ENABLE_SQL_LOGS": "false",
    "ENABLE_SWAGGER": "true",
    "MAIN_PORT": "8080"
  }
}
